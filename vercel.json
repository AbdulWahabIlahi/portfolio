{
  "rewrites": [
    { "source": "/", "destination": "/landingPage/index.html" },
    { "source": "/styles.css", "destination": "/landingPage/styles.css" },
    { "source": "/script.js", "destination": "/landingPage/script.js" },
    { "source": "/projects.js", "destination": "/landingPage/projects.js" },
    { "source": "/img/(.*)", "destination": "/landingPage/img/$1" },
    { "source": "/(.*)", "destination": "/$1" }
  ],
  "trailingSlash": false,
  "headers": [
    {
      "source": "/projects.js",
      "headers": [
        { "key": "Content-Type", "value": "application/javascript" }
      ]
    }
  ]
}
